{
  "name": "github-labeler",
  "description": "GitHub Action to manage labels on GitHub",
  "main": "lib/main.js",
  "scripts": {
    "build": "ncc build src/main.ts --source-map --minify --license licenses.txt",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts",
    "format": "eslint --fix src/**/*.ts __tests__/**/*.ts",
    "test": "jest --coverage",
    "all": "yarn run build && yarn run format && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazy-max/ghaction-github-labeler.git"
  },
  "keywords": [
    "actions",
    "github",
    "labels"
  ],
  "author": "CrazyMax",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/github": "^5.0.1",
    "@octokit/core": "^4.0.4",
    "@probot/octokit-plugin-config": "^1.1.5",
    "deepmerge": "^4.2.2",
    "js-yaml": "^4.1.0",
    "matcher": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vercel/ncc": "^0.34.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "jest": "^28.1.3",
    "nock": "^13.2.9",
    "prettier": "^2.3.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
